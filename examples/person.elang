class Person < Object
  def age
    @age
  end
  def age=(age)
    @age = age
  end
end

class Programmer < Person
  def lang
    @lang
  end
  def lang=(lang)
    @lang = lang
  end
end


p1 = Person.new
p1.age = 32
puts "p1.age = " + p1.age.to_s

p2 = Programmer.new
p2.age = 21
p2.lang = "Ruby"
puts "p2.age = " + p2.age.to_s
puts "p2.lang = " + p2.lang
